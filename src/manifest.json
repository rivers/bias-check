{
  "name": "Bias Check",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Shows media bias in your Facebook news feed",

  "content_scripts": [{
    "css": ["content.css"],
    "js": [
      "vendor/jquery-3.1.1.min.js",
      "vendor/psl.min.js",
      "content.js"
    ],
    "matches": ["*://*.facebook.com/*"]
  }],

  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "web_accessible_resources": ["images/icon-16.png"],

  "author": "Heather Rivers",
  "homepage_url": "https://github.com/rivers/bias-check",
  "minimum_chrome_version": "40",
  "permissions": ["<all_urls>"]
}
